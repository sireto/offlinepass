{
  "name": "Offline Pass",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Offline Pass",
    "default_popup": "index.html"
  },
  "short_name": "Offline Pass",
  "description": "Self Service Password Manager",
  "icons": {
    "16": "/icon.png",
    "32": "/icon.png",
    "150": "/icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Open Offline Pass"
    }
  },
  "permissions": [
    "activeTab"
  ]
}